{
    "name": "patrixsmart/skyriver",
    "description": "A progressive backend implementation of Laravel authentication system with bells and whistle.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Duruamadi",
            "email": "patrickadvance@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patrixsmart\\Skyriver\\SkyriverServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Patrixsmart\\Skyriver\\": "src/",
            "Patrixsmart\\Skyriver\\Database\\Factories\\Socialite\\": "database/factories/socialite",
            "Patrixsmart\\Skyriver\\Database\\Seeders\\Socialite\\": "database/seeders/socialite"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
